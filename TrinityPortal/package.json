{
  "name": "trinity_village",
  "version": "1.0.0",
  "description": "Trinity Village portal aims to provide a platform to manage the resources and services for robot services",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "generate-api-docs": "swagger-jsdoc -d ./controllers/Song.js -o ./swagger.json",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "azure": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build",
    "build": " npm run build --prefix client"
  },
  "author": "Khanh Nguyen",
  "license": "MIT",
  "dependencies": {
    "@azure/core-rest-pipeline": "^1.12.2",
    "@azure/msal-browser": "^3.10.0",
    "@azure/msal-node": "^2.6.4",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.7.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "install": "^0.13.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mssql": "^7.1.3",
    "nodemailer": "^6.6.2",
    "npm": "^10.5.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react-html-parser": "^2.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "xss": "^1.0.15",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  }
}
